# Web Application Environment Variables

# App Configuration
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=http://localhost:4000

# Request Configuration
VITE_REQUEST_TIMEOUT=30000
VITE_MAX_RETRIES=3

# AI Models
VITE_CLAUDE_MODEL=claude-sonnet-4-5
VITE_GEMINI_MODEL=gemini-2.5
VITE_CODEX_MODEL=codex-5.2

# AI Endpoints (Optional - for custom deployments)
# VITE_CLAUDE_ENDPOINT=https://api.anthropic.com
# VITE_GEMINI_ENDPOINT=https://generativelanguage.googleapis.com
# VITE_CODEX_ENDPOINT=https://api.openai.com

# Feature Flags
VITE_ENABLE_METRICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_SWAGGER=false
